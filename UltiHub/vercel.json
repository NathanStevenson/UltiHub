{
    "builds": [
      {
        "src": "UltiHub/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "UltiHub/wsgi.py"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}